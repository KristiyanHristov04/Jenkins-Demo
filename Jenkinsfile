pipeline {
    agent any

    stages {
        stage('Checkout') {
            steps {
                echo 'Checking out code...'
            }
        }
        stage('Build') {
            steps {
                echo 'Simulating HTML build...'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deployment complete!'
            }
        }
    }
}